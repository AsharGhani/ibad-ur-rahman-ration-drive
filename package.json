{
  "name": "",
  "description": "A site for a charity organization. The site is based off of a gatsby starter called gatsby-starter-typescript-plus.",
  "version": "1.0.0",
  "private": true,
  "author": "Ashar Ghani",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "dev:env": "./node_modules/.bin/env-cmd -f .env gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.12",
    "@svgr/webpack": "^4.3.2",
    "babel-plugin-styled-components": "^1.10.1",
    "classnames": "^2.2.6",
    "gatsby": "^2.21.33",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-canonical-urls": "^2.3.1",
    "gatsby-plugin-emotion": "^4.3.1",
    "gatsby-plugin-google-analytics": "^2.3.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sass": "^2.3.1",
    "gatsby-plugin-sharp": "^2.6.3",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-remark-copy-linked-files": "^2.3.2",
    "gatsby-remark-images": "^3.3.3",
    "gatsby-remark-prismjs": "^3.5.1",
    "gatsby-remark-responsive-iframe": "^2.4.2",
    "gatsby-remark-smartypants": "^2.3.1",
    "gatsby-source-contentful": "^2.3.5",
    "gatsby-source-filesystem": "^2.3.3",
    "gatsby-transformer-json": "^2.4.2",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.5.2",
    "modern-normalize": "^0.5.0",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.1",
    "prism-themes": "^1.1.0",
    "prismjs": "^1.16.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.8",
    "@types/node": "^12.0.10",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "env-cmd": "^9.0.3",
    "eslint": "^6.0.0",
    "eslint-config-blvd": "^0.1.1",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "react-responsive-carousel": "^3.1.49",
    "rimraf": "^2.6.3"
  }
}
